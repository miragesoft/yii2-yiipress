{
    "name": "miragesoft/yii2-yiipress",
    "description": "Yee CMS Based on Yii 2 Advanced Project Template",
    "keywords": ["yii2","extension","yiipress","cms"],
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Mirage Studio TH",
            "email": "miragestudioth@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/miragesoft/yii2-yiipress.git",
        "issues": "https://github.com/miragesoft/yii2-yiipress/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "miragesoft/yii2-user": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
