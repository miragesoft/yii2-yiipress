{
    "name": "miragesoft/yii2-yiipress",
    "version": "dev-master",
    "description": "CMS based on Yii2 Framework - Development package",
    "keywords": ["yiipress"],
    "homepage": "https://github.com/miragesoft/yii2-yiipress",
    "type": "yii2-extension",
    "license": "GNU GPL V3",
    "minimum-stability": "dev",
    "support": {
        "source": "https://github.com/miragesoft/yii2-yiipress.git",
        "issues": "https://github.com/miragesoft/yii2-yiipress/issues"
    },
    "authors": [
        {
            "name": "Mirage Studio TH",
            "email": "miragestudioth@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "miragesoft/yii2-user": "*",
    },
    "autoload": {
        "psr-4": {
            "mirage\\yiipress\\": "/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
